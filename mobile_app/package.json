{
  "name": "ryden",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "tsc": "tsc",
    "clean": "rimraf artifacts",
    "relay": "relay-compiler --src ./build --schema ../api_server/src/graphql/schema.graphql --extensions ts tsx js jsx",
    "relay:watch": "npm run relay -- --watch",
    "lint": "tslint src/**/*.ts",
    "build": "npm run clean && npm run tsc --",
    "build:watch": "npm run build -- -w",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "start:ios": "npm run build && concurrently -r 'npm run relay:watch' 'npm run build:watch' 'react-native run-ios --simulator=\"iPhone X\"'",
    "start:android": "npm run build && concurrently -r 'npm run relay:watch' 'npm run build:watch' 'react-native run-android'",
    "test": "jest --coverage"
  },
  "dependencies": {
    "@types/jest": "^22.2.2",
    "@types/lodash": "^4.14.106",
    "@types/react": "^16.1.0",
    "@types/react-native": "^0.52.22",
    "@types/react-relay": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-native-stage-0": "^1.0.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "lodash": "^4.17.5",
    "react": "16.3.0",
    "react-native": "0.54.4",
    "relay-runtime": "^1.5.0"
  },
  "devDependencies": {
    "babel-jest": "22.4.3",
    "babel-plugin-relay": "^1.5.0",
    "babel-preset-react-native": "4.0.0",
    "concurrently": "^3.5.1",
    "jest": "22.4.3",
    "react-relay": "^1.5.0",
    "react-test-renderer": "16.3.0",
    "relay-compiler": "^1.5.0",
    "rimraf": "^2.6.2",
    "tslint": "^5.9.1",
    "typescript": "^2.7.2"
  },
  "jest": {
    "preset": "react-native",
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "text-summary",
      "html"
    ],
    "collectCoverageFrom": [
      "build/**/*.js",
      "!build/**/*.spec.js",
      "!build/**/*.index.js"
    ]
  }
}
